{
  "name": "climb-on",
  "version": "1.0.0",
  "description": "Web app version of the Backyard Birding board game.",
  "main": "index.js",
  "homepage": "https://github.com/cdallasanta/climb-on#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": "10.16.2",
    "npm": "6.10.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "echo \"Error: no test specified\" && exit 1",
