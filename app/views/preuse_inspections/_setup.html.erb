<%= form_for @inspection, url: element_preuse_inspection_path(@element, @inspection)  do |f| %>
  Date: <%= f.date_field :date %>

  <%= f.fields_for @setup do |ff| %>
    <%= ff.check_box :equipment_complete %>
    <%= ff.check_box :environment_complete %>
    <%= ff.check_box :element_complete %>
  <% end %>

  <%= f.submit %>
<% end %>

<%# TODO this format is gross. Put it in a div %>
<i>
Updated by:<br>
<% @setup.users.each do |user| %>
  <%= user.fullname %><br>
<% end %>
</i>
