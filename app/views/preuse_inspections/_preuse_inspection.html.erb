<%= form_for inspection, url: edit_element_preuse_inspection_path(inspection.element, inspection), method: :get, :html=> {:id => 'preuse-inspection-form'}  do |f| %>
  <%= errors_check(inspection) %>

  <fieldset disabled>
    <div class="form-group">
      Date: <%= f.date_field :date, class:"form-control-sm" %><br>
    </div>
    <%= render partial: 'setup', locals: {setup: inspection.setup, f: f} %>
    <%= takedown_partial_if_exists(f) %>
  </fieldset>

  <%= f.submit "Edit this inspection" %>

<% end %>
